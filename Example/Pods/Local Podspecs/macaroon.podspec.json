{
  "name": "Macaroon",
  "version": "0.1.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/Hipo/macaroon",
  "summary": "iOS UIKit extension framework for providing structural interface functionality",
  "source": {
    "git": "https://github.com/Hipo/macaroon.git",
    "tag": "0.1.0"
  },
  "authors": {
    "Hipo": "hello@hipolabs.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "default_subspecs": "Production",
  "subspecs": [
    {
      "name": "Production",
      "dependencies": {
        "Macaroon/Core": [

        ]
      }
    },
    {
      "name": "Development",
      "dependencies": {
        "Macaroon/Core": [

        ],
        "Macaroon/Tryouts": [

        ],
        "Macaroon/SwiftLint": [

        ]
      }
    },
    {
      "name": "Core",
      "dependencies": {
        "SnapKit": [
          "~> 5.0.0"
        ]
      },
      "subspecs": [
        {
          "name": "Application",
          "source_files": "macaroon/Classes/Application/*.swift"
        },
        {
          "name": "Error",
          "source_files": "macaroon/Classes/Error/*.swift"
        },
        {
          "name": "Screens",
          "source_files": "macaroon/Classes/Screens/*.swift",
          "subspecs": [
            {
              "name": "Configuration",
              "subspecs": [
                {
                  "name": "NavigationBar",
                  "source_files": "macaroon/Classes/Screens/Configuration/NavigationBar/*.swift"
                },
                {
                  "name": "StatusBar",
                  "source_files": "macaroon/Classes/Screens/Configuration/StatusBar/*.swift"
                }
              ]
            },
            {
              "name": "Containers",
              "source_files": "macaroon/Classes/Screens/Containers/*.swift"
            }
          ]
        },
        {
          "name": "Utils",
          "source_files": "macaroon/Classes/Utils/*.swift",
          "subspecs": [
            {
              "name": "Extensions",
              "subspecs": [
                {
                  "name": "Foundation",
                  "source_files": "macaroon/Classes/Utils/Extensions/Foundation/*.swift"
                },
                {
                  "name": "UI",
                  "source_files": "macaroon/Classes/Utils/Extensions/UI/*.swift"
                }
              ]
            },
            {
              "name": "Notification",
              "source_files": "macaroon/Classes/Utils/Notification/*.swift"
            },
            {
              "name": "Text",
              "source_files": "macaroon/Classes/Utils/Text/*.swift",
              "subspecs": [
                {
                  "name": "Attributed",
                  "source_files": "macaroon/Classes/Utils/Text/Attributed/*.swift"
                }
              ]
            }
          ]
        },
        {
          "name": "Views",
          "source_files": "macaroon/Classes/Views/*.swift",
          "subspecs": [
            {
              "name": "Custom",
              "source_files": "macaroon/Classes/Views/Custom/*.swift"
            },
            {
              "name": "Layout",
              "source_files": "macaroon/Classes/Views/Layout/*.swift"
            },
            {
              "name": "Styling",
              "source_files": "macaroon/Classes/Views/Styling/*.swift"
            },
            {
              "name": "ViewModel",
              "source_files": "macaroon/Classes/Views/ViewModel/*.swift"
            }
          ]
        }
      ]
    },
    {
      "name": "SwiftLint",
      "dependencies": {
        "SwiftLint": [
          "~> 0.37.0"
        ]
      }
    },
    {
      "name": "TabBar",
      "source_files": "macaroon/Classes/TabBar/*.swift"
    },
    {
      "name": "Tryouts",
      "source_files": "macaroon/Classes/Tryouts/*.swift",
      "dependencies": {
        "Tryouts": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
